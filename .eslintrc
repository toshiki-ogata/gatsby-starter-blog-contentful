{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": ["prettier", "react", "jsx-a11y", "import"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "func-names": 0,
    "no-underscore-dangle": 0,
    "strict": 0,
    "react/no-danger": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js"]  }]
  }
}